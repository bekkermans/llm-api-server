http_options: 
  host: 0.0.0.0
  port: 7000
applications:
  - name: API
    import_path: api:app_builder
    deployments:
    - name: API
      num_replicas: 1
      ray_actor_options:
        num_gpus: 1
    args:
      llms:
        completions:
          - name: "/models/vicuna-7b"
            class: "vicuna"
        embedding:
          - name: "/models/all-MiniLM-L12-v2"
            class: "sentence-transformers"
          - name: "/models/all-MiniLM-L6-v2"
            class: "sentence-transformers"